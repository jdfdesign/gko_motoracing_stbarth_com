<% cache(["admin", @resource_locale.to_s, record]) do %>
	<td><%= glink_to(record.labelize, url_for([:edit, :admin, current_site, parent, record])) %></td>
	<td><%= indivisible_number_to_currency record.price %></td>
	<%= content_tag_for_actions do  %>
	    <%= icon_to_edit(record)  %>
	    <%= icon_to_destroy(record) %>
	<% end  %>
<% end  %>