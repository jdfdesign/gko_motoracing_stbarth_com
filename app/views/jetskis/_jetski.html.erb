<% collection_row_cache(jetski) do %>
<article class="col-xs-6 col-sm-4 thumbnail-item">
  <div class="thumbnail">
  <%= link_to(jetski.public_url) do %>
    <%= image_tag(jetski.thumbnail.thumbnail("450x325#").url(), :alt => jetski.thumbnail.title, :class => "img-responsive" ) if jetski.thumbnail %>
    <div class="caption">
      <h4 class="thumbnail-title"><%= jetski.title %></h4>
      <div class="thumbnail-price"><%= indivisible_number_to_currency(jetski.price) %></div>
    </div>
    <div class="wrap"></div>
    <i class="icon-zoom-in icon-2x"></i> 
  <% end %>
  </div>
</article>
<% end if jetski %>

